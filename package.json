  "scripts": {
    "start": "node dist/server.js",
    "dev": "npm run start:server",
    "dev:frontend": "vite --config frontend/vite.config.ts",
    "dev:backend": "ts-node server.ts",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:contract": "tsoa spec",
    "build:frontend": "vite build --config frontend/vite.config.ts",
    "build": "rm -rf dist && npm run build:frontend && npm run build:server",
    "build:server": "npm run build:contract && tsc",
    "start:server": "npm run build && npm start",
    "serve": "npm run build && npm start",
    "test": "react-scripts test",
    "test:db": "ts-node scripts/test-db.ts",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
